{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:flowtype/recommended",
    "google",
  ],
  "plugins": [
    "import",
    "react",
    "flowtype",
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "strict": 0,
    "no-console": 0,
    "linebreak-style": [
      "error",
      "windows"
    ],
  },
}